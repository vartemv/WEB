generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id      Int    @id @default(autoincrement())
  item    String @db.VarChar(255)
  name    String @db.VarChar(100)
  address String @db.VarChar(255)
  item_id String @db.VarChar(16)

  @@map("orders")
}
